{
  "name": "redux-tutorial",
  "version": "1.0.0",
  "description": "learning redux",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "test": "browserify test.js | smokestack | tap-spec",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/data-doge/redux-tutorial"
  },
  "author": "data-doge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/data-doge/redux-tutorial/issues"
  },
  "homepage": "https://github.com/data-doge/redux-tutorial",
  "dependencies": {
    "expect": "^1.20.2",
    "jquery": "^3.0.0",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "smokestack": "^3.3.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0",
    "uglifyify": "^3.0.1",
    "watchify": "^3.1.0"
  }
}
