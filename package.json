{
  "name": "react-redux-browserify-todo-app",
  "version": "1.0.0",
  "description": "learning redux",
  "main": "index.js",
  "scripts": {
    "start": "budo . -d --pushstate --serve bundle.js --live",
    "build": "browserify . -o bundle.js -g uglifyify",
    "dist": "mkdir -p dist && bash -c 'cp *.{js,html,css} dist'",
    "push": "gh-pages -d dist",
    "deploy": "npm run build && npm run dist && npm run push"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "stage-2",
            "react",
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/data-doge/react-redux-browserify-todo-app"
  },
  "author": "data-doge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/data-doge/react-redux-browserify-todo-app/issues"
  },
  "homepage": "https://github.com/data-doge/react-redux-browserify-todo-app",
  "dependencies": {
    "lodash.remove": "^4.5.0",
    "node-uuid": "^1.4.7",
    "normalizr": "^2.2.1",
    "object-key-filter": "0.0.4",
    "react": "^15.2.0",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0-alpha.1",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "babelify": "^7.3.0",
    "browserify": "^10.0.0",
    "budo": "^4.0.0",
    "gh-pages": "^0.3.0",
    "uglifyify": "^3.0.1"
  }
}
